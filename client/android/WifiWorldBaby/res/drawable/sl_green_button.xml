<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="0.0dip" />
            <solid android:color="@color/green_button_disable_color" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="0.0dip" />
            <solid android:color="@color/green_button_pressed_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="0.0dip" />
            <solid android:color="@color/green_button_default_color" />
        </shape>
    </item>
</selector>